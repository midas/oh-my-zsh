#!/bin/zsh
alias git-st='git status'
alias git-br='git branch'
alias git-br-contains='git branch --contains'
alias git-cm-amend='git commit --amend'
alias git-cleanup-older-than-day='git reflog expire --expire=1440.minute refs/heads/master; git fsck --unreachable; git prune; git gc'
alias git-push='git push'
alias git-push-origin='git push origin'
alias git-push-origin-masters='git push origin master'
alias git-push-origin-master-tags='git push origin master --tags'
alias git-push-tags='git push --tags'
alias git-pull='git pull --rebase'
alias git-pull-origin-master='git pull --rebase origin master'
alias git-co='git checkout'
alias git-co-master='git checkout master'
alias git-co-branch='git checkout -b'
alias git-co-track='git checkout --track'
alias git-cm='git commit'
alias git-ai='git add -i'
alias gai='git add -i'
alias git-rebase-master='git rebase master'
alias git-rebase-master-i='git rebase master -i'
alias git-log-pretty="git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
alias git-log-pretty2="git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit -p"
alias git-log-decorate='git log --decorate'
alias git-log-decorate-oneline='git log --oneline --decorate'
alias git-log-decorate-graph-all='git log --decorate --graph --all'
alias git-log-decorate-graph-all-oneline='git log --decorate --graph --all --oneline'
alias git-log-follow='git log --follow -p'
alias git-log-follow-worddiff='git log --word-diff --follow -p'
alias git-log-search-filename='git log --'
alias git-log-search-filename-withdiff='git log -p --'
alias git-log-showdiffs='git log -p'
alias git-log-showdiffs-nameonly='git log --name-status'
alias git-log-showdiffs-status='git log --stat'
alias git-log-unpushed='git log origin..HEAD'
alias git-log-unpushed-withdiff='git log -p origin..HEAD'
alias git-bisect-start='git bisect start'
alias git-bisect-good='git bisect good'
alias git-bisect-bad='git bisect bad'
alias git-bisect-reset='git bisect reset'
alias git-difftool='git difftool'
alias git-mergetool='git mergetool'
alias git-difftool-opendiff='git difftool -t opendiff'
alias git-mergetool-opendiff='git mergetool -t opendiff'
alias git-difftool-diffmerge='git difftool -t diffmerge'
alias git-mergetool-diffmerge='git mergetool -t diffmerge'
alias git-namerev='git name-rev'
alias git-namerev-nameonly='git name-rev --name-only'
alias gfilen='git log -n'
alias git-diff='git diff'
alias git-diff-worddiff='git diff --word-diff'
alias git-whatchanged='git whatchanged'
alias git-whatchanged-oneline='git whatchanged --oneline'
